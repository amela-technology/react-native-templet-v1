{
    "name": "DemoApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "init-project": "react-native eject && npm run pod-install",
        "start": "react-native start",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "test": "jest",
        "pod-install": "cd ios && pod install",
        "postinstall": "cd scripts && sh ./fix-lib.sh && cd .. && cd ios && pod install && cd .. && npx jetifier",
        "assets-link": "npx react-native link",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "devtools": "react-devtools",
        "no-edit": "git add . && git commit --amend --no-edit && git push origin HEAD -f",
        "no-verify": "git add . && git commit --amend --no-edit --no-verify && git push origin HEAD -f",
        "rebase": "git checkout develop && git pull origin develop && git checkout - && git rebase -",
        "buildAndroid": "cd android && ./gradlew assembleRelease&& adb -d install app/build/outputs/apk/release/app-release.apk",
        "bundleIOS": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "open-folder-apk": "open ./android/app/build/outputs/apk",
        "android-run-debug-develop": "react-native run-android --no-jetifier --variant=devDebug --appId 'jp.honeything.app.dev'",
        "android-run-debug-staging": "react-native run-android --no-jetifier --variant=stagingDebug --appId 'jp.honeything.app.stg'",
        "android-run-debug-product": "react-native run-android --no-jetifier --variant=productDebug --appId 'jp.honeything.app.prod'",
        "android-run-release-develop": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --no-jetifier --variant=devRelease --appId 'jp.honeything.app.dev'",
        "android-run-release-staging": "react-native run-android --no-jetifier --variant=stagingRelease --appId 'jp.honeything.app.stg'",
        "android-run-release-product": "react-native run-android --no-jetifier --variant=productRelease --appId 'jp.honeything.app.prod'",
        "android-build-apk-develop": "rm -rf build && rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
        "android-build-apk-staging": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-build-apk-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductRelease && cd ..",
        "android-build-aab-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductRelease && cd ..",
        "code-push-ios-develop": "appcenter codepush release-react -a Amela/project-name -d Development -t 1.0.0",
        "code-push-ios-staging": "appcenter codepush release-react -a Amela/project-name -d Staging -t 1.0.0",
        "code-push-ios-production": "appcenter codepush release-react -a Amela/project-name -d Production -t 1.0.0",
        "code-push-android-develop": "appcenter codepush release-react -a Amela/project-name -d Development -t 1.0.0",
        "code-push-android-staging": "appcenter codepush release-react -a Amela/project-name -d Staging -t 1.0.0",
        "code-push-android-production": "appcenter codepush release-react -a Amela/project-name -d Production -t 1.0.0",
        "prepare": "husky install",
        "push-develop": "yarn code-push-ios-develop && yarn code-push-android-develop",
        "push-staging": "yarn code-push-ios-staging && yarn code-push-android-staging"
    },
    "dependencies": {
        "@hookform/resolvers": "2.6.0",
        "@react-native-community/async-storage": "^1.12.1",
        "@react-native-community/masked-view": "^0.1.11",
        "@react-navigation/bottom-tabs": "^5.11.11",
        "@react-native-community/netinfo": "^6.0.0",
        "@react-navigation/native": "^5.9.4",
        "@react-navigation/stack": "^5.14.5",
        "ahooks": "^2.10.8",
        "@reduxjs/toolkit": "^1.6.0",
        "axios": "^0.21.1",
        "crypto": "^1.0.1",
        "dayjs": "^1.10.6",
        "eslint-config-airbnb": "^18.2.1",
        "hash-object": "^0.1.7",
        "i18next": "^20.3.2",
        "lodash": "^4.17.21",
        "react": "17.0.2",
        "react-hook-form": "7.10.1",
        "react-i18next": "^11.11.1",
        "react-native": "0.64.2",
        "react-native-actionsheet": "^2.4.2",
        "react-native-aws3": "^0.0.9",
        "react-native-code-push": "^7.0.1",
        "react-native-config": "^1.4.2",
        "react-native-confirmation-code-input": "^1.0.4",
        "react-native-gesture-handler": "^1.10.3",
        "react-native-image-crop-picker": "^0.36.2",
        "react-native-keyboard-aware-scroll-view": "^0.9.4",
        "react-native-localize": "^2.1.1",
        "react-native-modal": "^12.0.2",
        "react-native-modalize": "^2.0.8",
        "react-native-onesignal": "^4.1.1",
        "react-native-permissions": "^3.0.5",
        "react-native-picker": "^4.3.7",
        "react-native-portalize": "^1.0.7",
        "react-native-reanimated": "^2.2.0",
        "react-native-root-siblings": "^4.1.1",
        "react-native-safe-area-context": "^3.2.0",
        "react-native-screens": "^3.4.0",
        "react-native-size-matters": "^0.4.0",
        "react-native-static-safe-area-insets": "^2.1.1",
        "react-native-webview": "^11.6.5",
        "react-redux": "^7.2.4",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.3",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/core": "^7.14.6",
        "@babel/runtime": "^7.14.6",
        "@react-native-community/eslint-config": "^3.0.0",
        "@types/jest": "^26.0.24",
        "@types/react-native": "^0.64.12",
        "@types/react-redux": "^7.1.18",
        "@types/react-test-renderer": "17.0.1",
        "@types/redux-logger": "^3.0.9",
        "@typescript-eslint/eslint-plugin": "^4.28.2",
        "@typescript-eslint/eslint-plugin-tslint": "^4.28.2",
        "@typescript-eslint/parser": "^4.28.2",
        "babel-jest": "^27.0.6",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.30.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-import-resolver-typescript": "^2.4.0",
        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jsdoc": "^35.4.3",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^3.4.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-unused-imports": "^1.1.2",
        "husky": "^7.0.1",
        "jest": "^27.0.6",
        "metro-react-native-babel-preset": "^0.66.1",
        "prettier": "^2.3.2",
        "react-devtools": "^4.13.5",
        "react-native-eject": "^0.1.2",
        "react-test-renderer": "17.0.2",
        "typescript": "^4.3.5"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    }
}
