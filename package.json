{
    "name": "DemoApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "init-project": "react-native eject && npm run pod-install",
        "start": "react-native start",
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "test": "jest",
        "codepush": "./code-push.command",
        "pod-install": "cd ios && pod install && cd ..",
        "postinstall": "patch-package && yarn pod-install",
        "prepare": "husky install",
        "assets-link": "npx react-native link",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:ts": "tslint 'src/**/*.{ts,tsx,js}'",
        "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
        "lint:husky": "eslint $(git diff --staged --name-only | grep -E '(.js$|.ts$|.tsx$)')",
        "devtools": "react-devtools",
        "get-tree": "tree -I 'android|ios|node_modules'",
        "no-edit": "git add . && git commit --amend --no-edit && git push origin HEAD -f",
        "no-verify": "git add . && git commit --amend --no-edit --no-verify && git push origin HEAD -f",
        "rebase": "git checkout develop && git pull origin develop && git checkout - && git rebase -",
        "buildAndroid": "cd android && ./gradlew assembleRelease&& adb -d install app/build/outputs/apk/release/app-release.apk",
        "bundleIOS": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
        "android-run-debug-develop": "react-native run-android --no-jetifier --variant=devDebug --appId 'com.amela.vn.dev'",
        "android-run-debug-staging": "react-native run-android --no-jetifier --variant=stagingDebug --appId 'com.amela.vn.stg'",
        "android-run-debug-product": "react-native run-android --no-jetifier --variant=productDebug --appId 'com.amela.vn.prod'",
        "android-run-release-develop": "cd android && rm -rf build && rm -rf android/build && cd .. && react-native run-android --no-jetifier --variant=devRelease --appId 'com.amela.vn.dev'",
        "android-run-release-staging": "react-native run-android --no-jetifier --variant=stagingRelease --appId 'com.amela.vn.stg'",
        "android-run-release-product": "react-native run-android --no-jetifier --variant=productRelease --appId 'com.amela.vn.prod'",
        "android-build-apk-develop": "rm -rf build && rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew assembleDevRelease && cd ..",
        "android-build-apk-staging": "rm -rf build && rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-build-apk-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew assembleProductRelease && cd ..",
        "android-build-aab-product": "rm -rf build && rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew clean && ./gradlew bundleProductRelease && cd ..",
        "open-folder-apk": "open ./android/app/build/outputs/apk",
        "open-folder-aab": "open ./android/app/build/outputs/bundle",
        "commit": "cz"
    },
    "dependencies": {
        "@alessiocancian/react-native-actionsheet": "3.2.0",
        "@hookform/resolvers": "3.0.0",
        "@react-native-community/async-storage": "1.12.1",
        "@react-native-community/cli": "10.2.2",
        "@react-native-community/datetimepicker": "7.0.0",
        "@react-native-community/masked-view": "0.1.11",
        "@react-native-community/netinfo": "9.3.7",
        "@react-navigation/bottom-tabs": "6.5.7",
        "@react-navigation/native": "6.1.6",
        "@react-navigation/stack": "6.3.16",
        "@reduxjs/toolkit": "1.9.3",
        "@socket.io/component-emitter": "4.0.0",
        "ahooks": "3.7.5",
        "axios": "1.3.4",
        "crypto": "1.0.1",
        "dayjs": "1.11.7",
        "deprecated-react-native-prop-types": "^4.0.0",
        "eslint-config-airbnb": "19.0.4",
        "hash-object": "0.1.7",
        "i18next": "22.4.13",
        "lodash": "4.17.21",
        "react": "18.2.0",
        "react-fast-compare": "3.2.1",
        "react-hook-form": "7.43.7",
        "react-i18next": "12.2.0",
        "react-native": "0.71.4",
        "react-native-aws3": "0.0.9",
        "react-native-code-push": "7.1.0",
        "react-native-config": "1.5.0",
        "react-native-confirmation-code-input": "1.0.4",
        "react-native-gesture-handler": "2.9.0",
        "react-native-image-crop-picker": "0.39.0",
        "react-native-keyboard-aware-scroll-view": "0.9.5",
        "react-native-localize": "2.2.6",
        "react-native-modal": "13.0.1",
        "react-native-modal-datetime-picker": "14.0.1",
        "react-native-modal-dropdown": "1.0.2",
        "react-native-modalize": "2.1.1",
        "react-native-onesignal": "4.5.1",
        "react-native-permissions": "3.8.0",
        "react-native-picker": "4.3.7",
        "react-native-portalize": "1.0.7",
        "react-native-progress": "5.0.0",
        "react-native-reanimated": "3.0.2",
        "react-native-restart": "0.0.27",
        "react-native-root-siblings": "4.1.1",
        "react-native-safe-area-context": "4.5.0",
        "react-native-screens": "3.20.0",
        "react-native-size-matters": "0.4.0",
        "react-native-static-safe-area-insets": "2.2.0",
        "react-native-svg-charts": "5.4.0",
        "react-native-webview": "11.26.1",
        "react-redux": "8.0.5",
        "reactotron-react-native": "5.0.3",
        "reactotron-redux": "3.1.3",
        "redux-logger": "3.0.6",
        "redux-persist": "6.0.0",
        "redux-saga": "1.2.3",
        "socket.io-client": "4.6.1",
        "yup": "1.0.2"
    },
    "devDependencies": {
        "@babel/core": "7.21.3",
        "@babel/preset-env": "7.20.2",
        "@babel/runtime": "7.21.0",
        "@commitlint/cli": "17.5.0",
        "@commitlint/config-conventional": "17.4.4",
        "@react-native-community/eslint-config": "3.2.0",
        "@tsconfig/react-native": "2.0.3",
        "@types/jest": "29.5.0",
        "@types/react": "18.0.28",
        "@types/react-native": "0.71.4",
        "@types/react-redux": "7.1.25",
        "@types/react-test-renderer": "18.0.0",
        "@types/redux-logger": "3.0.9",
        "@typescript-eslint/eslint-plugin": "5.56.0",
        "@typescript-eslint/eslint-plugin-tslint": "5.56.0",
        "@typescript-eslint/parser": "5.56.0",
        "babel-jest": "29.5.0",
        "babel-plugin-module-resolver": "5.0.0",
        "commitizen": "4.3.0",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.36.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-import-resolver-typescript": "3.5.3",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "40.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "husky": "8.0.3",
        "jest": "29.5.0",
        "metro-react-native-babel-preset": "0.76.0",
        "patch-package": "6.5.1",
        "prettier": "2.8.6",
        "react-devtools": "4.27.3",
        "react-native-eject": "0.2.0",
        "react-test-renderer": "18.2.0",
        "typescript": "5.0.2"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
